{
    "manifest_version": 3,
    "name": "Random Text Generator",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "commands": {
        "generate-random-text": {
            "suggested_key": {
                "default": "Ctrl+M"
            },
            "description": "Generate random text"
        },
        "random": {
            "suggested_key": {
                "default": "Alt+Shift+L"
            },
            "description": "Load a random link"
        }
    }
}